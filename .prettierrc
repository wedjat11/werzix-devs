{
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "printWidth": 200,
  "htmlWhitespaceSensitivity": "ignore",
  "proseWrap": "always",
  "plugins": ["prettier-plugin-tailwindcss"]
}
